{
  "name": "blog-front",
  "version": "1.0.0",
  "description": "vue blog project",
  "author": "David Meir-Levy <davidmeirlevy@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "npm run set-theme && nuxt build",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "set-theme": "node ./helpers/set-theme.js",
    "generate": "npm run set-theme && nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/component-cache": "^1.1.5",
    "@vue/composition-api": "^0.5.0",
    "cross-env": "^5.2.1",
    "express": "^4.16.4",
    "node-sass": "^4.13.1",
    "nuxt": "^2.12.0",
    "sass-loader": "^8.0.2",
    "vuex": "^3.1.2",
    "vuex-composition-helpers": "^1.0.16"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
